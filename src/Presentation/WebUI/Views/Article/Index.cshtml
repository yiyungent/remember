@using Domain.Entities
@model Article
@{
	Layout = "~/Views/Shared/_LayoutMain.cshtml";
	ViewBag.Title = Model.Title;
}

<div class="row">
	<div class="col-xs-12 text-center">
		<h1>@Model.Title</h1>
	</div>
</div>
<div class="row">
	<div class="col-xs-12">
		@Html.Raw(Model.Content)
	</div>
</div>
